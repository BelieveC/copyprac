<div id="page_wrapper">

	<% if @college.errors.any?%>
		<div id="errors">
			<h2><%= pluralize(@college.errors.count, "Error")%> prevented this college from saving</h2>
			<ul>
				<% @college.errors.full_messages.each do |msg| %>
					<li><%= msg%></li>
				<% end%>
			</ul>
		</div>
	<% end %>
	<h4 id="head"> New College</h4>
	<%=render('form')%>
	<br>
	<%=link_to "Back",:back,class:"button"%>
</div>
